{
  "name": "telegram-heroku-client",
  "version": "1.0.0",
  "description": "Unofficial Heroku Client for Telegram",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start",
    "build": "rollup --config rollup.config.ts --configPlugin typescript"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x",
    "yarn": "1.x"
  },
  "keywords": [
